{
    "name":  "microkids-backend",
    "version":  "0.1.0",
    "description":  "Backend para Agente Financeiro Microkids",
    "author":  "",
    "private":  true,
    "license":  "UNLICENSED",
    "scripts":  {
                    "start":  "node dist/main.js",
                    "start:dev":  "ts-node -r tsconfig-paths/register src/main.ts",
                    "build":  "tsc -p tsconfig.build.json",
                    "lint":  "eslint \"src/**/*.ts\"",
                    "prisma:generate":  "prisma generate",
                    "prisma:migrate":  "prisma migrate dev",
                    "import:excel":  "ts-node -r tsconfig-paths/register src/finance/tasks/import-excel.ts",
                    "convert:excel":  "ts-node -r tsconfig-paths/register src/finance/tasks/excel-to-json.ts",
                    "import:json":  "ts-node -r tsconfig-paths/register src/finance/tasks/json-to-db.ts",
                    "sync:all":  "npm run convert:excel \u0026\u0026 npm run import:json",
                    "postinstall":  "prisma generate",
                    "start:prod":  "node dist/main.js"
                },
    "dependencies":  {
                         "@nestjs/common":  "^10.3.0",
                         "@nestjs/core":  "^10.3.0",
                         "@nestjs/platform-express":  "^10.3.0",
                         "@nestjs/config":  "^3.2.0",
                         "@nestjs/schedule":  "^4.0.0",
                         "@prisma/client":  "^5.7.1",
                         "class-transformer":  "^0.5.1",
                         "class-validator":  "^0.14.0",
                         "dotenv":  "^16.3.1",
                         "exceljs":  "^4.4.0",
                         "nodemailer":  "^6.9.7",
                         "reflect-metadata":  "^0.1.13",
                         "rxjs":  "^7.8.1",
                         "twilio":  "^5.10.6"
                     },
    "devDependencies":  {
                            "@nestjs/cli":  "^10.2.1",
                            "@nestjs/schematics":  "^10.0.3",
                            "@nestjs/testing":  "^10.3.0",
                            "@types/express":  "^4.17.21",
                            "@types/node":  "^20.10.6",
                            "@types/nodemailer":  "^6.4.14",
                            "prisma":  "^5.7.1",
                            "source-map-support":  "^0.5.21",
                            "ts-loader":  "^9.5.1",
                            "ts-node":  "^10.9.2",
                            "tsconfig-paths":  "^4.2.0",
                            "typescript":  "^5.3.3"
                        }
}
